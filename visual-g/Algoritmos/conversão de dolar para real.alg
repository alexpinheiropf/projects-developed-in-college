algoritmo "semnome"
// Função :
// Autor :
// Data : 03/09/2014
// Seção de Declarações 
var
  reais,dolar,cotacao : real
inicio
   Escreval("Digite a cotação atual do dolar: ")
   leia(cotacao)
   Escreval("Qual o valor de dólares a ser convertido em reais")
   Leia(dolar)
   
   reais <- dolar * cotacao
   escreval("o Valor do resultado é: R$",reais)
   
   
fimalgoritmo
