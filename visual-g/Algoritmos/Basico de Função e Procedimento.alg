algoritmo "semnome"
// Função :
// Autor :

funcao somaNumero(num1,num2 : inteiro ) : inteiro

var
   somar : inteiro
inicio
   somar <- (num1 + num2)
   retorne somar

fimfuncao

procedimento lerNumero()

var
   x,y : inteiro
inicio
    leia(x)
    escreva()
    leia (y)
    escreva()
    
    escreva(somaNumero(x,y))
fimprocedimento

var

inicio
 lerNumero()
fimalgoritmo
