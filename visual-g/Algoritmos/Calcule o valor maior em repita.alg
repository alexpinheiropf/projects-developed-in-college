algoritmo "semnome"
// Função :
// Autor :
// Data : 24/09/2014
// Seção de Declarações 
var
  x, n, maior : inteiro
inicio

   repita
      Escreval("digite um numero:")
      leia(n)
       se n > x  entao
        maior <- n
        fimse
    ate (n = 0)
    escreval("o valor maior", maior)
        
   

fimalgoritmo
